#include <stdio.h>

int main()
{
    int t;
    scanf("%d", &t);
    while (t--)
    {
        int n;
        scanf("%d", &n);
        char arr[n];
        int count = 0;
        int i;

        for (i=0; i<=n; i++)
        {
            scanf("%c", &arr[i]);
        }

        for (int i = 0; i <= n; i++)
        {
            if (arr[i] =='0' || arr[i] =='5')
            {
                count++;
        }
        }
        if (count>=1)
        {
            printf("Yes\n");
        }
        else
        {
            printf("No\n");
        }
    }

    return 0;
}
