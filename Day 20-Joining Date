#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n,k;
	    scanf("%d %d",&n,&k);
	    if(n%5==0 && k%5!=0)
	    {
	        printf("%d\n",(n/5-k/5-1));
	    }
	    else if(n%5!=0 && k%5==0)
	    {
	        printf("%d\n",(n/5-k/5+1));
	    }
	    else
	    {
	        printf("%d\n",(n/5-k/5));
	    }

	}
	return 0;
}

