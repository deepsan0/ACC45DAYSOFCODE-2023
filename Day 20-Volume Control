#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int x,y;
	    scanf("%d %d",&x,&y);
	    if(x>y)
	    {
	        printf("%d\n",(x-y));
	    }
	    else if(y>x)
	    {
	        printf("%d\n",(y-x));
	    }
	    else
	    {
	        printf("0\n");
	    }
	}
	return 0;
}

