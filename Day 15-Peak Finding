#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n,h;
	    scanf("%d",&n);
	    int arr[n],max=0;
	    for(int i=0;i<n;i++)
	    {
	       
	        scanf("%d\n",&arr[i]);
	        if(arr[i]>max)
	        {
	            max=arr[i];
	        }
	        
	    }
	    printf("%d\n",max);
	}
	return 0;
}

