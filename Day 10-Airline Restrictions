#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int a,b,c,d,e;
	    scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
	    if(d>=(a+b) && c<=e)
	    {
	        printf("yes\n");
	        
	    }
	    else if(d>=(a+c) && b<=e)
	    {
	        printf("yes\n");
	    }
	    else if(d>=(b+c) && a<=e)
	    {
	        printf("yes\n");
	    }
	    else
	    {
	        printf("no\n");
	    }
	}
	return 0;
}

