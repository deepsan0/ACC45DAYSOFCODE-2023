#include <stdio.h>

int main(void) {
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n,x,count=0;
        scanf("%d %d",&n,&x);
        int a[n];
        for(int i=0;i<n;i++)
        {
            scanf("%d",&a[i]);
            if(a[i]>=x)
            {
                count++;
            }
        }
        printf("%d\n",count);
        
    }
	return 0;
}

