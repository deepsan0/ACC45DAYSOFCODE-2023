#include <stdio.h>

int main(void) {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--)
    {
        int r1,w1,c1,r2,w2,c2;
        scanf("%d %d %d",&r1,&w1,&c1);
        scanf("%d %d %d",&r2,&w2,&c2);
        if((r1>r2 &&w1>w2)||(r1>r2 && c1>c2)||(c1>c2 &&w1>w2))
        {
            printf("A\n");
        }
        else
        {
            printf("B\n");
        }
        
    }
	return 0;
}

