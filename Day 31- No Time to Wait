#include <stdio.h>

int main(void) {
	// your code goes here
	int n,h,x;
	int count=0;
	scanf("%d%d%d",&n,&h,&x);
	int t[n];
	for(int i=0;i<n;i++)
	{
	    scanf("%d",&t[i]);
	    if(x+t[i]==h)
	    {
	        count++ ;
	    }
	    
	}
	if(count)
	{
	    printf("yes\n");
	}
	else
	{
	    printf("no\n");
	}
	return 0;
}
