#include <stdio.h>
int main() {
    int t,a,b,x,y;
    scanf("%d",&t);
    while(t--){
        scanf("%d %d %d %d",&a,&b,&x,&y);
        if (a==b){
            printf("yes\n");
        }
        else if(a>b && a-b<=y){
            printf("yes\n");
        }
        else if (a<b && b-a<=x){
            printf("yes\n");
            
        }
        else{
            printf("no\n");
        }
    }
	return 0;
}

